{
    "python.testing.unittestEnabled": false,
    "python.testing.pytestEnabled": true,
    "search.exclude": {
        "**/.git/**": true,
        "**/.idea/**": true,
        "**/build/**": true,
        "**/android": true,
        "**/macos": true,
        "**/web": true,
        "**/ios": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache/**": true,
        "**/.mypy_cache/**": true,
        "**/node_modules/**": true,
        "**/.dart_tool/**": true
    },
    "files.exclude": {
        "**/.git/**": true,
        "**/.idea/**": true,
        "**/build/**": true,
        "**/android": true,
        "**/macos": true,
        "**/web": true,
        "**/ios": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/Thumbs.db": true,
        "**/node_modules": true,
        "**/__pycache__": true,
        "**/.pytest_cache": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache/**": true,
        "**/.mypy_cache/**": true,
        "**/node_modules/**": true,
        "**/.dart_tool/**": true,
    },
    "files.watcherExclude": {
        "**/.pytest_cache/**": true,
        "**/.mypy_cache/**": true,
        "**/node_modules/**": true,
        "**/.dart_tool/**": true,
        "**/.venv/**": true,
        "**/.git/**": true,
        "**/.idea/**": true,
        "**/build/**": true,
        "**/android": true,
        "**/macos": true,
        "**/dist": true,
        "**/web": true,
        "**/ios": true,
        "**/.git": true
    },
    "python.analysis.typeCheckingMode": "off",
    "python.defaultInterpreterPath": "${workspaceFolder}/.venv/bin/python",
    "python.analysis.inlayHints.variableTypes": true,
    "python.analysis.inlayHints.parameterTypes": true,
    "python.analysis.inlayHints.returnType": true,
}